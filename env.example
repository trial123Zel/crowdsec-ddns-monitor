### Crowdsec container settings
CROWDSEC_API_KEY=   #Generated during crowdsec init
CROWDSEC_CF_BOUNCER_CF_API_KEY=   #if using cloudflare bouncer
CROWDSEC_CF_BOUNCER_CROWDSEC_API_KEY=   #if using CF bouncer, the Enroll key from Crowdsec

### DDNS-monitor container settings
# DDNS Configuration
DDNS_DOMAINS=   #comma separated list
CHECK_INTERVAL=300

# Crowdsec settings
ALLOWLIST_NAME=   #string
CF_ALLOWLIST_NAME=   #string
CROWDSEC_API_URL=http://crowdsec:8080

# Cloudflare settings
CF_PROXIED_DOMAINS=   #cloudflare domain name behind CF proxy
CF_IP_REFRESH_INTERVAL=86400

# DNS Settings
DNS_SERVERS=1.1.1.1,9.9.9.9
DNS_TIMEOUT=5
DNS_RETRIES=3

# Logging
LOG_LEVEL=INFO

# Alerting (optional)
ALERT_WEBHOOK_URL=   #works with Slack, Discord, others.

# Host identification
HOSTNAME=   #custom server host name